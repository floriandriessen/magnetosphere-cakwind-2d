!=================================================
! Project : 2.5D isothermal, magnetic CAK wind
!
! Aim     : start from a 1D relaxed CAK wind to
!           study wind-magnetosphere interaction
!
! Config  : setup.pl -d=2
!=================================================

&filelist
  base_filename = 'test/ms'
  saveprim      = .true.
  autoconvert   = .true.
  nwauxio       = 2
  !convert_type  = 'tecplotmpi'
/

&savelist
  itsave(1,1) = 0
  itsave(1,2) = 0
  ditsave_log = 1000
  dtsave_dat  = 2.5d-2
/

&stoplist
  dtmin    = 1.0d-20
  time_max = 0.5d0
/

&methodlist
  time_stepper = 'twostep'
  flux_scheme  = 3*'hll'
  limiter      = 3*'vanleer'
/

&boundlist
  ! rho, mom1, mom2, mom3, b1, b2, b3
  typeboundary_min1 = 7*'special'
  typeboundary_max1 = 7*'cont'
  typeboundary_min2 = 'symm','symm','asymm','asymm','symm','asymm','asymm'
  typeboundary_max2 = 'symm','symm','asymm','asymm','symm','asymm','asymm'
/

&meshlist
  domain_nx1     = 256 
  domain_nx2     = 128
  block_nx1      = 16 
  block_nx2      = 16 
  xprobmin1      = 1.0d0 
  xprobmax1      = 10.0d0 
  xprobmin2      = 0.0d0
  xprobmax2      = 0.5d0
  stretch_dim(1) = 'uni'
  qstretch_baselevel(1) = 1.02
/

&paramlist
  courantpar = 0.8d0 
/

&mhd_list
  mhd_gravity = .true.
  mhd_energy  = .false.
  mhd_gamma   = 1.0d0
  typedivbfix = 'powell'
  B0field     = .false.
  mhd_cak_force = .true.
/

&cak_list
  cak_alpha    = 0.65d0
  gayley_qbar  = 2000.0d0
  cak_1d_force = .true.
  cak_1d_opt   = 1
  cak_split    = .true.
  cak_vector_force = .false.
/

&star_list
  cakfile  = 'relaxed-CAK-winds/ic-zetapup-30rs.blk'
  mstar    = 50.0d0
  rstar    = 20.0d0
  twind    = 4.0d4
  etastar  = 10.0d0
  rhobound = 2.0d-11
  timestat = 2.0d5
  Wrot     = 0.01d0
/
